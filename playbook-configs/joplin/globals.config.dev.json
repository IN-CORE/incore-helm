				"dispatchKey": "RECEIVE_BLOCK_GROUP"
			}
		],
		"valueLosses": [
			{
				"description": "Value loss beta distribution parameters",
				"type": "incore:valuLossParam",
				"format": "table",
				"num": 5,
				"isSocioeconomics": true,
				"dispatchKey": "RECEIVE_VALUE_LOSS"
			}
		]
